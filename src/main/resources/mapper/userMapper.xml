<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="UserMapper">
        <select id="selectUser" resultType="java.util.Map">
            SELECT USER_INDEX
                 , USER_NAME
                 , ADMIN_LEVEL
              FROM USER_INFO
        </select>
</mapper>